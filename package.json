  "scripts": {
    "setup:infra-only": "docker compose -f docker/docker-compose.infra.yml up -d",
    "shutdown:infra-only": "docker compose -f docker/docker-compose.infra.yml down",
    "start:services": "nx run-many -t=serve -p gateway,catalog,order",
    "start:gateway": "nx serve gateway",
    "start:catalog": "nx serve catalog",
    "start:order": "nx serve order",
    "start:web": "nx dev web",
    "test:all": "nx run-many -t test --all",
    "prisma:generate": "cd libs/database && npx prisma generate",
    "prisma:migrate": "cd libs/database && npx prisma migrate dev",
    "prisma:deploy": "cd libs/database && npx prisma migrate deploy",
    "prisma:studio": "cd libs/database && npx prisma studio",
    "prisma:reset": "cd libs/database && npx prisma migrate reset",
    "prisma:seed": "cd libs/database && npx ts-node seed.ts"
  },
  "private": true,
